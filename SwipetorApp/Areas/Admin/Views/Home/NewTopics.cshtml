<script type="text/javascript">

$(function() {
	google.charts.setOnLoadCallback(drawWeeklyNewUsersChart);
});

function drawWeeklyNewUsersChart() {
	var data = google.visualization.arrayToDataTable([
	  ['Year', 'Sales', 'Expenses'],
	  ['2004',  1000,      400],
	  ['2005',  1170,      460],
	  ['2006',  660,       1120],
	  ['2007',  1030,      540]
	]);

	var options = {
	  title: 'Weekly new members',
	  curveType: 'function',
	  legend: { position: 'bottom' }
	};

	var chart = new google.visualization.LineChart(document.getElementById('weeklyNewUsersChart'));

	chart.draw(data, options);
}

</script>


<div id="weeklyNewUsersChart"></div>